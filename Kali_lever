
echo "[1] start kali user "
echo "[2] start root user "
read -p "why you login : " wimg
        if (( $wimg == "1" ));
        then
            wimg="kali"
        elif (( $wimg == "2" ));
        then
            wimg="root"
        elif (( $wimg == "3" ));
        then
            wimg="exit"
        else
            wimg="exit"
        fi
function kali () {
sudo -u kali /bin/bash
}
function root () {
echo
}
$wimg
